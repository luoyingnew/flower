.top-bar,.distribution{padding:10px 15px;box-sizing:border-box}.top-bar{top:0px;z-index:999;padding:15px}.top-bar .back{color:"#1989fa"}.distribution{position:relative;top:40px}.cart-list{margin-top:42px;margin-bottom:55px}.cart-list .item{height:100px;margin-top:2px;padding:5px 10px}.cart-list .item>view{height:100%}.cart-list .item image{width:70px}.cart-list .item .goods-info{height:100%}.cart-list .item .goods-info .price{color:#f40;font-weight:bold}.cart-list .item .goods-info .change-num{height:25px;border-radius:5px;background-color:#c4c4c4;line-height:25px}.cart-list .item .goods-info .change-num view:not(:nth-of-type(2)){height:100%;width:25px;text-align:center}.cart-list .item .goods-info .change-num view:nth-of-type(2){padding:0 15px;line-height:25px;border-left:1px solid #fff;border-right:1px solid #fff}.cart-list .item .goods-info .change-num .disabled{background-color:#e5e5e5;color:#bbb;border-top-left-radius:5px;border-bottom-left-radius:5px}.footer{height:50px;bottom:0px;padding:0 10px;box-sizing:border-box}.footer .priceAll{color:#f40;font-weight:bold}.footer .pay{height:40px;width:35%;color:#fff;text-align:center;line-height:40px;border-radius:20px;background:linear-gradient(to right, #ff6034, #ee0a24)}